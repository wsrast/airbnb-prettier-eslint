{
	"name": "@wsrast/airbnb-prettier-eslint",
	"version": "1.0.7",
	"author": "Wesley Rast",
	"description": "A boilerplate setup for linting using prettier, Airbnb settings and Git hooks.",
	"repository": {
		"type": "git",
		"url": "https://github.com/wsrast/airbnb-prettier-eslint"
	},
	"keywords": "prettier, airbnb, eslint",
	"scripts": {
		"postinstall": "node request.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"request": "^2.88.0"
	},
	"devDependencies": {
		"cross-env": "^5.2.0",
		"eslint": "^5.16.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-jest": "^22.4.1",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-react": "^7.12.4",
		"eslint-plugin-react-hooks": "^1.6.0",
		"husky": "^1.3.1",
		"jest": "^24.7.0",
		"jest-dom": "^3.1.3",
		"jest-in-case": "^1.0.2",
		"lint-staged": "^8.1.5",
		"prettier": "^1.16.4",
		"react-testing-library": "^6.0.4"
	},
	"peerDependencies": {
		"cross-env": "^5.2.0",
		"eslint": "^5.16.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-jest": "^22.4.1",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-react": "^7.12.4",
		"eslint-plugin-react-hooks": "^1.6.0",
		"husky": "^1.3.1",
		"jest": "^24.7.0",
		"jest-dom": "^3.1.3",
		"jest-in-case": "^1.0.2",
		"lint-staged": "^8.1.5",
		"prettier": "^1.16.4",
		"react-testing-library": "^6.0.4"
	}
}
